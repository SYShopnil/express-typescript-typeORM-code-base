{
  "name": "express-typeScript-typeORM-swagger-code-base",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:dist && npm run swagger && tsc && npm run build:ormconfig && npm run build:public",
    "build:production": "npm install && npm run build:dist && npm run swagger && tsc && npm run build:ormconfig && npm run build:public",
    "build:lib": "tsc",
    "build:dist": "mkdir dist",
    "build:ormconfig": "copyfiles -f ormconfig.js  dist/",
    "build:public": "copyfiles -f src/public/**/*  dist/public",
    "seed": "node  --max-old-space-size=8192 dist/services/seeder.js",
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "typeorm": "dotenv -e .env typeorm",
    "swagger": "npm run swagger:user && npm run swagger:auth",
    "swagger:user": "swagger-jsdoc -d src/swagger.user.json -o  dist/swagger.user.json src/swagger.ts",
    "swagger:auth": "swagger-jsdoc -d src/swagger.auth.json -o  dist/swagger.auth.json src/swagger.ts"
  },
  "keywords": [],
  "author": "chiragmehta900",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.8",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.15",
    "@types/pino": "^6.3.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^7.4.2",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.14.1",
    "class-validator-password-check": "^1.0.8",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "geojson": "^0.5.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.5",
    "mysql2": "^3.9.7",
    "nodemailer": "^6.7.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.2.37",
    "typeorm-pagination": "^2.0.3"
  }
}
